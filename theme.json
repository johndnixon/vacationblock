{
	"$schema": "https://schemas.wp.org/wp/6.1/theme.json",
	"customTemplates": [
		{
			"name": "page-image-header",
			"postTypes": [
				"page",
				"post"
			],
			"title": "Page (Image Header)"
		}
	],
	"settings": {
		"appearanceTools": true,
		"color": {
			"defaultGradients": false,
			"palette": [
				{
					"color": "#ffffff",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#000000",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#2e2e2e",
					"name": "Dark Gray",
					"slug": "dark-gray"
				},
				{
					"color": "#0D47A0",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#136AED",
					"name": "Primary Shade",
					"slug": "primary-shade"
				},
				{
					"color": "#03062f",
					"name": "Primary Shade Dark",
					"slug": "primary-shade-dark"
				},
				{
					"color": "#79cbff",
					"name": "Primary Shade Light",
					"slug": "primary-shade-light"
				},
				{
					"color": "#F56800",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#F6F6F6",
					"name": "Tertiary",
					"slug": "tertiary"
				}
			],
			"gradients": [
				{
					"gradient": "linear-gradient(135deg,var(--wp--preset--color--primary) 0%,var(--wp--preset--color--primary-shade-light) 100%)",
					"name": "Primary Shade",
					"slug": "primary-shade"
				}
			]
		},
		"layout": {
			"contentSize": "1145px",
			"wideSize": "1270px"
		},
		"spacing": {
			"spacingScale": {
				"steps": 0
			},
			"spacingSizes": [
				{
					"name": "1",
					"size": "clamp(1.5rem, 5vw, 2rem)",
					"slug": "30"
				},
				{
					"name": "2",
					"size": "clamp(1.8rem, 1.8rem + ((1vw - 0.48rem) * 2.885), 3rem)",
					"slug": "40"
				},
				{
					"name": "3",
					"size": "clamp(2.5rem, 8vw, 4.5rem)",
					"slug": "50"
				},
				{
					"name": "4",
					"size": "clamp(3.75rem, 10vw, 7rem)",
					"slug": "60"
				},
				{
					"name": "5",
					"size": "clamp(5rem, 5.25rem + ((1vw - 0.48rem) * 9.096), 8rem)",
					"slug": "70"
				},
				{
					"name": "6",
					"size": "clamp(7rem, 14vw, 11rem)",
					"slug": "80"
				}
			],
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"dropCap": false,
			"fluid": true,
			"fontFamilies": [
				{
					"fontFamily": "'figtree', sans-serif",
					"name": "Figtree",
					"slug": "figtree"
				}
			],
			"fontSizes": [
				{
					"size": "13px",
					"slug": "small"
				},
				{
					"fluid": false,
					"size": "15px",
					"slug": "medium"
				},
				{
					"fluid": false,
					"size": "20px",
					"slug": "large"
				},
				{
					"fluid": {
						"max": "4.5rem",
						"min": "2rem"
					},
					"size": "4.5rem",
					"slug": "x-large"
				},
				{
					"fluid": {
						"max": "10rem",
						"min": "4rem"
					},
					"size": "10rem",
					"slug": "xx-large"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/table": {
				"color": {
					"text": "var:preset|color|contrast"
				},
				"elements": {
					"heading": {
						"color": {
							"background": "#0D47A0",
							"text": "#ffffff"
						}
					}
				},
				"typography": {
					"fontFamily": "var:preset|font-family|figtree",
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontStyle": "normal",
					"fontWeight": "400"
				}
			}
		},
		"color": {
			"background": "var:preset|color|white"
		},
		"elements": {
			"button": {
				"border": {
					"radius": "0"
				},
				"color": {
					"background": "var(--wp--preset--color--secondary)",
					"text": "var(--wp--preset--color--white)"
				},
				"typography": {
					"fontSize": "0.875rem",
					"fontWeight": "400",
					"lineHeight": "1.2"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "34px",
					"lineHeight": "1.2"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "30px",
					"lineHeight": "1.2"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "248x"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "24px"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "20px",
					"textTransform": "uppercase"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "15px",
					"textTransform": "uppercase"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--figtree)",
					"fontWeight": "700"
				}
			}
		},
		"spacing": {
			"blockGap": "1.5rem",
			"padding": {
				"left": "var(--wp--preset--spacing--30)",
				"right": "var(--wp--preset--spacing--30)"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--figtree)",
			"fontSize": "var(--wp--preset--font-size--regular)",
			"lineHeight": "1.86"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		}
	],
	"version": 2
}